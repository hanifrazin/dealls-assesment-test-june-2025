  },
  "homepage": "https://github.com/hanifrazin/dealls-assesment-test-june-2025#readme",
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "cypress": "^14.4.1",
    "cypress-xpath": "^2.0.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0"
  },
  "dependencies": {
    "flatted": "^3.3.3"
  }
}
